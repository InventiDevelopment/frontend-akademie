<li class="transaction" data-toggle=".transaction-expand">
  <div>
    <div class="transaction-name">{{ name }}</div>
    <div class="transaction-created">{{dateFormat created }}</div>
    <div class="transaction-value">{{ value}}</div>
  </div>
  <div class="transaction-expand">
    <div class = "transaction-buttons">
      <div class="delete-button">
        <button></button>
      </div>
      <div class="action-button edit-button">
        <button></button>
      </div>
    </div>
  </div>
</li>